{
  "name": "bal-ui-library",
  "version": "0.0.1",
  "description": "Lightweight UI-Component Library for Baloise",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/bal-ui-library/bal-ui-library.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs-readme",
    "watch": "stencil build --watch --docs-readme",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "docs": "docsify serve ./ --open",
    "start": "concurrently \"npm:watch\" \"npm:docs\""
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@stencil/core": "^1.3.3",
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^1.0.1",
    "@types/jest": "^24.0.18",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.2",
    "docsify": "^4.9.4",
    "docsify-copy-code": "^2.1.0",
    "jest": "^24.9.0",
    "mermaid": "^8.3.1"
  },
  "license": "MIT",
  "dependencies": {
    "bulma": "^0.7.5"
  }
}
